<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle" >
            <solid android:color="#11006AFE" />
            <corners
                android:topRightRadius="@dimen/wh_10"
                android:bottomRightRadius="@dimen/wh_10" />
        </shape>
    </item>
    <item android:right="0dp" android:left="-3dp">
        <shape>
            <corners
                android:topRightRadius="@dimen/wh_10"
                android:bottomRightRadius="@dimen/wh_10" />
            <stroke
                android:width="1dp"
                android:color="@color/color_orange" />
        </shape>
    </item>
</layer-list>